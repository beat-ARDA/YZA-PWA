{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/*.ts"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "apiSQL",
      "urls": [
        "https://apim-canibalizacion-poc-eastus2-01.azure-api.net/**"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "1d",
        "timeout": "1h",
        "strategy": "freshness"
      },
      "cacheQueryOptions": {
        "ignoreSearch": false
      }
    }
  ]
}